		<attribute>
			<name>menuId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>yearMonth</name>
		<tagclass>com.takucin.aceeci.taglib.YearMonthTag</tagclass>
		<attribute>
			<name>yearName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>monthName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
